{
    "manifest_version": 2,
    "name": "Shruti to eBay Lister",
    "description": "List Shruti products to eBay",
    "version": "1.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.shruticreation.com/*",
                "*://shruticreation.com/*"
            ],
            "js": [
                "libs/jquery-3.2.1.min.js",
                "content.js"
            ]
        }
    ]
}